{
  "name": "react-environment",
  "version": "0.0.0",
  "description": "Dev environment",
  "author": "Chris Dobson",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "build": "webpack --config ./webpack.config.prod.js --hide-modules --progress --colors",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "postbuild": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools --ext .js --ext .jsx",
    "test": "jest",
    "lint:watch": "yarn run lint --watch",
    "test:watch": "yarn run test --watch"
  },
  "dependencies": {
    "colors": "1.1.2",
    "config": "^1.29.4",
    "prop-types": "^15.6.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-react-hmre": "1.1.1",
    "compression": "1.6.2",
    "css-loader": "0.27.3",
    "duplicate-package-checker-webpack-plugin": "^2.1.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.0.1",
    "expect": "1.20.2",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "jest-fetch-mock": "^1.4.2",
    "jest-junit": "^3.6.0",
    "jsdom": "9.12.0",
    "npm-run-all": "4.0.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "^16.5.0",
    "rimraf": "2.6.1",
    "style-loader": "0.14.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "0.5.8",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "2.17.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
